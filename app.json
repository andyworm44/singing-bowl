{
  "expo": {
    "name": "送缽",
    "slug": "singing-bowl",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.andy0608.singingbowl",
      "buildNumber": "1",
      "supportsTablet": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": ["audio"],
        "CFBundleDisplayName": "送缽",
        "CFBundleName": "送缽"
      }
    },
    "android": {
      "package": "com.andy0608.singingbowl",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "5f6c79e7-02c3-49f3-b3cb-69d3a9d26844"
      }
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "singing-bowl"
  }
}
